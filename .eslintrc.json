{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "linebreak-style": ["off"],
    "max-len": ["error", { "code": 300 }],
    "no-underscore-dangle": ["off"],
    "no-nested-ternary": ["off"]
  }
}
